{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "@tsconfig/node18/tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "moduleResolution": "Bundler"
  }
}
