{
  "collectionName": "",
  "info": {
    "name": "user",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "unique": true
    },
    "provider": {
      "type": "string"
    },
    "password": {
      "type": "password",
      "minLength": 6
    },
    "resetPasswordToken": {
      "type": "string"
    },
    "role": {
      "type": "integer"
    },
    "posts": {
